{{ form_start(form) }}
<div class="row">
    <div class="col-md-6">{{ form_row(form.title) }}</div>
    <div class="col-md-6">{{ form_row(form.address) }}</div>
</div>
<div class="row">
    <div class="col-md-6">{{ form_row(form.contact) }}</div>
    <div class="col-md-6">{{ form_row(form.email) }}</div>
</div>
{{ form_rest(form) }}
<button class="btn btn-primary">{{ button | default('Enregistrer') }}</button>
{{ form_end(form) }}